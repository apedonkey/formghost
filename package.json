{
  "name": "formghost",
  "version": "2.1.0",
  "description": "FormGhost Chrome Extension",
  "scripts": {
    "start": "node formghost-site/server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:stress": "jest --testPathPattern=tests/stress --runInBand --testTimeout=300000",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:load": "node tests/load/aiLoadTest.js",
    "test:memory": "node tests/memory/memoryProfiler.js",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "test:watch": "jest --watch",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:stress",
    "profile:memory": "node --expose-gc tests/memory/memoryProfiler.js",
    "report": "open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/chrome": "^0.0.254",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "puppeteer": "^21.6.0",
    "autocannon": "^7.14.0",
    "clinic": "^13.0.0"
  },
  "keywords": [
    "chrome-extension",
    "automation",
    "testing",
    "formghost"
  ],
  "author": "FormGhost",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
